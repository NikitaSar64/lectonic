.rolepage__flex{
    margin-top: 16px;
    display: flex;
    overflow-x: auto;
    white-space: nowrap;
}

.rolepage__flex::-webkit-scrollbar-thumb{
  background: #B4B4B4;
  border-radius: 18px;
}

.rolepage__flex::-webkit-scrollbar {
  height: 6px;
  background-color: #DBDBDB;
  border-radius: 18px;
}
